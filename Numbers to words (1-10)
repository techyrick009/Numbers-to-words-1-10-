#include<stdio.h>

int main(void) {

	int number;

	printf("Enter a number (1-10) : ");
	scanf("%d", &number);

	if (number == 1)
		printf("One");
	if (number == 2)
		printf("Two");
	if (number == 3)
		printf("Three");
	if (number == 4)
		printf("Four");
	if (number == 5)
		printf("Five");
	if (number == 6)
		printf("Six");
	if (number == 7)
		printf("Seven");
	if (number == 8)
		printf("Eight");
	if (number == 9)
		printf("Nine");
	if (number == 10
		)
		printf("Ten");

	return 0;
}


